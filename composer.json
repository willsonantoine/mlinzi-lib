{
    "name": "Mlinzi-lib",
    "description": "Librairie de Mlinzi corp",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "vulembere",
            "email": "willsonantoine@gmail.com",
            "homepage": "http://www.mlinzi-corporation.com/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "tuupola/slim-basic-auth": "^3.2",
        "firebase/php-jwt": "^5.0",
        "tuupola/slim-jwt-auth": "^3.2",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "OurOwnNamespace\\": "src/classes"
        }
    }
}